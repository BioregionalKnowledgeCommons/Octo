# KOI Processor Configuration
# Copy to personal.env and fill in values

# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=octo_koi
DB_USER=postgres
DB_PASSWORD=<generate-a-strong-password>

# OpenAI (for semantic entity resolution)
OPENAI_API_KEY=sk-...
EMBEDDING_MODEL=text-embedding-3-small

# Vault
VAULT_PATH=/root/.openclaw/workspace/vault

# Mode
MODE=personal
